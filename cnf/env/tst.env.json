{
    "general_comment": "use BIG_CAPS to indicate that the vars are exported !!!"
  , "env_type_comment": "the environment type - dev -> development , stg -> staging , prd-> production"
  , "interpolation_comment": "the declaration order matters, already declared vars are interpolated"
  , "multi_lines_comment": "to indicate new line use \\n, to indicate 3 spaces \\t"
  , "env": {
        "ENV_TYPE": "tst"
      , "app": {
           "ht_protocol": "http"
         , "ws_protocol": "ws"
         , "web_host": "host-name"
         , "port": "3002"
         , "mojo_morbo_port": "3001"
         , "mojo_daemon_port": "441"
         , "mojo_hypnotoad_port": "8081"
         , "postgres_db_name": "tst_qto"
      }
      , "db": {
           "postgres_db_name": "tst_qto"
         , "postgres_db_host": "127.0.0.1"
         , "postgres_db_port": "15432"
         , "postgres_db_user": "usrqtoapp"
         , "postgres_db_user_pw": "usrqtoapp"
         , "postgres_db_useradmin": "usrqtoadmin"
         , "postgres_db_useradmin_pw": "usrqtoadmin"
      }
      , "ssh": {
           "ssh_server": "qto.fi"
         , "ssh_user": "ubuntu"
         , "ssh_key_pair_file": "~/.ssh/key-pair-qto-amzn.pem"
         , "launch_dir": "/home/ubuntu/dat/tmp"
      }
      , "log": {
         "LogToFile": 1
         , "LogDir": "$ProductInstanceDir/dat/log"
         , "LogFile": "$LogDir/$ProductName.%YYYY%.%MM%.%DD%.log"
         , "PrintConsoleMsgs": 1
         , "PrintInfoMsgs": 1                             
         , "PrintWarningMsgs": 1                             
         , "PrintErrorMsgs": 1                             
         , "PrintDebugMsgs": 1
         , "PrintTraceMsgs": 1 
         , "TimeFormat" : "YYYY-MM-DD hh:mm:ss"
         , "LogTimeToTextSeparator":":::"
      }
      , "AdminEmail": "yordan.georgiev@gmail.com"
	}
}
