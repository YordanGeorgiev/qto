# the name of the mysql user 
mysql_user = usrisgpuben

# the pw of the mysql user
mysql_user_pw = secret

# where are the mysql scripts situated
mysql_scripts_dir = /opt/csitea/issue-tracker/issue-tracker.0.3.7.prd.ysg/src/sql/mysql/prd_issue_tracker

